{
  "WillLuke.nextjs.hasPrompted": true,
  "cSpell.words": [
    "Aperturs",
    "Bachu",
    "datepicker",
    "trpc",
    "unknow",
    "zustand"
  ],
  "editor.formatOnSave": true,
